{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "static/**/*.ttl",
        "watchAssets": true,
        "outDir": "dist/src/"
      },
      {
        "include": "static/**/*.json",
        "watchAssets": true,
        "outDir": "dist/src/"
      },
      {
        "include": "static/**/*.md",
        "watchAssets": true,
        "outDir": "dist/src/"
      },
      {
        "include": "static/**/*.pem",
        "watchAssets": true,
        "outDir": "dist/src/"
      },
      {
        "include": "secrets/*.pem",
        "watchAssets": true,
        "outDir": "dist/src/"
      }
    ],
    "watchAssets": true
  }
}